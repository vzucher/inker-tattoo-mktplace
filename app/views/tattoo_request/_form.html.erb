<%= simple_form_for(tattoo_request) do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :bodypart %>
<%= f.association :specialties, collection: Specialty.all.order(:name) %>
<%= f.input :references_photos, as: :file %>
<%= f.button :submit, "Submit", class: "btn btn-primary" %>
<% end %>
